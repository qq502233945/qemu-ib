    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/qs/p5800/qemu/build/virtiofsd",
             "args": [ 
                "-o","vhost_user_socket=/tmp/vhostqemu",
                "-o","source=/home/qs/p5800/Share",
                "-o","cache=always",
                "-d",
                    ],
            "sudo": true,
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
